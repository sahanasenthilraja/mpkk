{
  "manifest_version": 3,
  "name": "NeoExamShield",
  "version": "1.0.6",
  "description": "To prevent malpractice, identifies and blocks third-party browser extensions during tests on the Iamneo portal. Also provides tools for AI responses, custom copy/paste, and keeping the window active.",
  "background": {
    "service_worker": "worker.js"
  },
"web_accessible_resources": [{
    "resources": [
        "devtools.js",
        "data/inject/asj2524sa.js",
        "data/inject/b3c5d7e9.js",
        "data/inject/x9y2z4a1.js",
        "data/inject/d5e6f7g8.js",
        "data/inject/j5k6l7m8.js",
        "data/inject/n9o0p1q2.js",        
        "data/inject/z1a2b3c4.js",
        "data/inject/a1b2c3d4.js",
        "data/inject/e5f6g7h8.js",
        "data/inject/i9j0k1l2.js",
        "data/inject/m3n4o5p6.js",
        "data/inject/q7r8s9t0.js",
        "data/inject/r3s4t5u6.js",
        "data/inject/v7w8x9y0.js",
        "metadata.json",
        "background.js",
        "contentScript.js",
        "manifest.json",
        "data/inject/mock_code/manifest.json",
        "data/inject/mock_code/minifiedBackground.js",
        "data/inject/mock_code/minifiedContent-script.js",
        "data/inject/mock_code/rules.json"
    ],
    "matches": ["http://*/*", "https://*/*"]
}],
  "permissions": [
    "management",
    "tabs",
    "activeTab",
    "contextMenus",   
    "clipboardWrite",
    "clipboardRead",
    "storage",
    "scripting",
    "downloads",
    "windows",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "alarms",
    "webRequest"
  ],
  "host_permissions": [
      "*://*/*"
  ],
  "key": "",
  "externally_connectable": {
     "matches": ["http://*/*", "https://*/*"]
   },
  "action": {
    "default_popup": "popup1.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "data/inject/asj2524sa.js"
      ],
      "run_at": "document_start",
      "world": "MAIN",
      "all_frames": true
    },
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "data/inject/n9o0p1q2.js",
        "data/inject/a1b2c3d4.js",
        "data/inject/i9j0k1l2.js",
        "data/inject/m3n4o5p6.js",
        "data/inject/d5e6f7g8.js",
        "data/inject/q7r8s9t0.js",    
        "contentScript.js"
      ],
      "exclude_matches": [
        "*://*.discord.com/*",
        "*://*.figma.com/*"
      ]
    },
    {
      "matches": [
          "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
          "devtools.js"
      ]
    },
    {
      "js": ["data/inject/b3c5d7e9.js"],
      "run_at": "document_start",
      "all_frames": true,
      "matchOriginAsFallback": true,
      "world": "MAIN",
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "js": ["data/inject/sdsaas2325.js"],
      "run_at": "document_start",
      "all_frames": true,
      "matchOriginAsFallback": true,
      "world": "MAIN",
      "matches": [
        "<all_urls>",
        "https://*.examly.io/*",
        "https://*.vit.ac.in/*",
        "https://app.codility.com/*"
      ],
      "exclude_matches": ["https://*.examly.io/result*","https://*/result"]
    }    
  ],
  "commands": {
    "search-mcq": {
      "suggested_key": {
        "default": "Alt+Shift+M",
        "mac": "Alt+Shift+M"
      },
      "description": "Solve MCQs"
    },
    "search": {
      "suggested_key": {
        "default": "Alt+Shift+S",
        "mac": "Alt+Shift+S"
      },
      "description": "Solve MCQs"
    },
    "customPaste": {
      "suggested_key": {
        "default": "Alt+Shift+V",
        "mac": "Alt+Shift+V"
      },
      "description": "Paste using drag-and-drop when blocked"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "version_name": "Release Version"
}